# Salesforce認証情報
# 実際の値を入力して、このファイルを .env にコピーしてください

# Salesforceのユーザー名
SF_USERNAME=your_username@example.com

# Salesforceのパスワード
SF_PASSWORD=your_password

# Salesforceのセキュリティトークン（OAuth認証を使用する場合は省略可）
SF_SECURITY_TOKEN=your_security_token

# Salesforceのドメイン（本番環境の場合は 'login'、サンドボックスの場合は 'test'）
SF_DOMAIN=login

# OAuth 2.0認証（SOAP APIが無効な場合に使用）
# Connected Appから取得したConsumer KeyとSecret
# これらを設定すると、OAuth 2.0認証が優先されます
SF_CONSUMER_KEY=your_consumer_key
SF_CONSUMER_SECRET=your_consumer_secret
